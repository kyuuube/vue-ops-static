{
    "name": "vue-ops-static",
    "version": "1.0.0",
    "main": "index.js",
    "author": "benchiu<benchiu@yandex.com>",
    "license": "MIT",
    "scripts": {
        "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
        "build": "webpack --inline --progress --config build/webpack.pord.conf.js",
        "eslint": "eslint --ext js,jsx,ts,tsx src",
        "stylelint": "stylelint 'src/**/*.less' --syntax less",
        "fix": "prettier --write 'src/**/*.{tsx,jsx,js,ts,less}'",
        "dll": "webpack --config build/webpack.config.dll.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{jsx,js,vue}": [
            "yarn run eslint",
            "git add"
        ],
        "src/**/*.{scss}": [
            "yarn run stylelint",
            "git add"
        ],
        "src/**/*.{js,vue,less,jsx}": [
            "prettier --write 'src/**/*.{jsx,js,less, vue}'",
            "git add"
        ]
    },
    "dependencies": {
        "@babel/core": "^7.5.5",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "axios": "^0.21.1",
        "babel-eslint": "^10.0.2",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-component": "^1.1.1",
        "babel-plugin-dynamic-import-webpack": "^1.1.0",
        "babel-plugin-import": "^1.13.0",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "clean-webpack-plugin": "^3.0.0",
        "compression-webpack-plugin": "^3.1.0",
        "copy-webpack-plugin": "^5.0.4",
        "css-loader": "^3.4.2",
        "element-ui": "^2.15.1",
        "eslint": "^6.1.0",
        "eslint-friendly-formatter": "^4.0.1",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-vue": "^5.2.3",
        "file-loader": "^5.1.0",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "github-webhook-handler": "^1.0.0",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^4.2.5",
        "insert-css": "^2.0.0",
        "less-loader": "^5.0.0",
        "lint-staged": "^10.2.2",
        "md5": "^2.2.1",
        "mem": "^6.1.0",
        "mini-css-extract-plugin": "^0.9.0",
        "moment": "^2.24.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-cssnext": "^3.1.0",
        "prettier": "^1.19.1",
        "prosemirror-utils": "^0.9.6",
        "style-loader": "^1.1.3",
        "stylus-loader": "^3.0.2",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "url-loader": "^3.0.0",
        "vue": "^2.6.10",
        "vue-loader": "^15.7.1",
        "vue-progressbar": "^0.7.5",
        "vue-quill-editor": "^3.0.6",
        "vue-router": "^3.1.2",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.10",
        "vuex": "^3.1.1",
        "webpack": "^4.39.2",
        "webpack-bundle-analyzer": "^3.4.1",
        "webpack-cli": "^3.3.6",
        "webpack-dev-server": "^3.8.0",
        "webpack-merge": "^4.2.1",
        "webpackbar": "^4.0.0"
    },
    "devDependencies": {
        "deepmerge": "^4.2.2",
        "fibers": "^4.0.2",
        "less": "^3.11.1",
        "sass": "^1.25.0",
        "sass-loader": "^8.0.2"
    }
}
